{
  "name": "mochallel",
  "version": "1.6.3",
  "description": "Mocha tests running in parallel",
  "main": "lib/index.js",
  "scripts": {
    "test": "istanbul cover -x '**/vendor/**' ./node_modules/.bin/_mocha --require=babel-register",
    "prepublish": "babel src -d lib && babel vendor -d lib/vendor",
    "prepare": "npm run prepublish --silent",
    "precommit": "npm run lint --silent && npm run test --silent",
    "lint": "eslint --fix 'src/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabiosantoscode/mochallel.git"
  },
  "directories": {
    "bin": "bin",
    "lib": "lib"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fabiosantoscode/mochallel/issues"
  },
  "homepage": "https://github.com/fabiosantoscode/mochallel#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "es6-promise": "^4.2.5",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.1.2",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.0.0",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.4.1",
    "circular-json": "^0.5.7",
    "commander": "^2.19.0"
  }
}
