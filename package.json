{
  "name": "mochallel",
  "version": "1.7.5",
  "description": "Mocha tests running in parallel",
  "keywords": [
    "mocha",
    "parallel",
    "testing",
    "test"
  ],
  "main": "lib/index.js",
  "scripts": {
    "test": "istanbul cover -x '**/vendor/**' ./node_modules/.bin/_mocha && coveralls < coverage/lcov.info",
    "prepare": "babel src -d lib",
    "precommit": "npm run lint --silent && npm run test --silent",
    "lint": "eslint --fix 'src/**/*.js' --ignore-pattern 'vendor'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabiosantoscode/mochallel.git"
  },
  "directories": {
    "bin": "bin",
    "lib": "lib"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fabiosantoscode/mochallel/issues"
  },
  "homepage": "https://github.com/fabiosantoscode/mochallel#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "es6-promise": "^4.2.5",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.1.2",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.4.1",
    "commander": "^2.19.0",
    "coveralls": "^3.0.2",
    "multiprocess-map": "^1.3.2",
    "semver": "^5.6.0"
  }
}
